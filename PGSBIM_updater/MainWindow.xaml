        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PGSBIM_updater"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="350" Loaded="Load">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="254*"/>
            <ColumnDefinition Width="31*"/>
            <ColumnDefinition Width="450*"/>
            <ColumnDefinition Width="65*"/>
        </Grid.ColumnDefinitions>
        <TextBlock HorizontalAlignment="Left" Margin="75,75,0,0" TextWrapping="Wrap" Text="PGS-BIM уведомление об обновлениях" VerticalAlignment="Top" Grid.ColumnSpan="3"/>
    </Grid>
</Window>
